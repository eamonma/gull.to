{
  "name": "gull-to-redirect",
  "version": "0.1.0",
  "description": "Bird species redirect system for gull.to/g/* paths",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "npm run etl && tsc --noEmit",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy --env production",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "etl": "tsx src/etl/build-mapping.ts",
    "map:version": "tsx scripts/detect-map-version.ts",
    "synthetic:staging": "tsx scripts/synthetic-check.ts https://staging.gull.to",
    "synthetic:prod": "tsx scripts/synthetic-check.ts https://gull.to",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "ci": "npm run validate && npm run build"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240909.0",
    "d3-dsv": "^3.0.1"
  },
  "devDependencies": {
    "@types/d3-dsv": "^3.0.7",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "wrangler": "^4.34.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
